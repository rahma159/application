  <property name="styleSheet">
   <string notr="true">/* Main Window Styling */
QWidget {
    background-color: #f8f9fa; /* Light gray background for a clean, modern look */
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    color: #212529; /* Darker text for better readability */
}

/* Labels */
QLabel {
    font-weight: bold;
    color: #003366; /* Deep blue for headers */
    font-size: 15px;
    padding: 4px ;
}

/* Input Fields */
QLineEdit {
    border: 2px solid #3399ff; /* Blue border for input fields */
    border-radius: 8px; /* Rounded corners for modern design */
    padding: 8px 10px;
    min-height: 30px;
    background-color: #ffffff; /* White background */
    color: #495057;
    transition: all 0.3s ease; /* Smooth transition for hover/focus effects */
}
QLineEdit:hover {
    border: 2px solid #007acc;
    background-color: #f1f8ff; /* Slightly lighter blue background */
}
QLineEdit:focus {
    outline: none;
    border: 2px solid #0056b3;
    box-shadow: 0 0 5px rgba(0, 86, 179, 0.5); /* Subtle glow effect */
}

/* Group Box */
QGroupBox {
    border: 2px solid #3399ff;
    border-radius: 10px;
    margin-top: 20px;
    font-size: 30px;
    background-color: #ffffff;
    padding: 10px;
}
QGroupBox:title {
    subcontrol-origin: margin;
    left: 15px;
    padding: 0 8px;
    color: #003366;
    font-size: 18px;
    font-weight: bold;
    background-color: #f8f9fa;
    border: 2px solid #3399ff;
	border-radius: 5px;
}

/* Buttons */
QPushButton {
    background-color: #007bff; /* Blue button */
    color: white;
    border: none;
    border-radius: 8px;
    padding: 8px 16px;
    font-weight: bold;
    font-size: 14px;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1); /* Subtle shadow */
    transition: all 0.3s ease; /* Smooth transition for hover/pressed effects */
}
QPushButton:hover {
    background-color: #0056b3; /* Darker blue on hover */
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2); /* Slightly larger shadow */
}
QPushButton:pressed {
    background-color: #003d80; /* Even darker blue for pressed state */
    box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.2); /* Inset shadow to indicate press */
}

/* Table Headers */
QHeaderView::section {
    background-color: #3399ff; /* Blue header background */
    color: white;
    font-weight: bold;
    font-size: 14px;
    padding: 8px;
    border: none;
    text-align: left;
    border-radius: 5px;
}

/* Table View */
QTableView {
    border: 2px solid #dee2e6; /* Subtle border for table */
    border-radius: 8px;
    background-color: #ffffff; /* White table background */
    gridline-color: #dee2e6; /* Light gray grid lines */
    selection-background-color: #007bff; /* Blue selection */
    selection-color: white;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    color: #212529;
}
QTableView::item:hover {
    background-color: #f1f8ff; /* Light blue hover effect */
}
QTableView::item:selected {
    background-color: #0056b3; /* Dark blue for selected item */
    color: white;
}

/* Scrollbars */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #e9ecef; /* Light gray scrollbar background */
    border: none;
    width: 10px;
    height: 10px;
    margin: 0px;
    border-radius: 5px;
}
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #007bff; /* Blue scrollbar handle */
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    background: #0056b3; /* Darker blue on hover */
}
QScrollBar::add-line, QScrollBar::sub-line {
    background: none;
}</string>
